{
   "name": "@see/upload-service",
   "version": "1.0.0",
   "private": true,
   "repository": {
      "type": "git",
      "url": "https://github.com/Kbgjtn/see"
   },
   "main": "dist/index.js",
   "watch": [
      "src/"
   ],
   "ignore": [
      ".git",
      "node_modules/",
      "coverage/"
   ],
   "ext": "js,json,ts",
   "license": "UNLICENSED",
   "homepage": "https://see.coocobolo.com",
   "scripts": {
      "build": "tsc -p tsconfig-build.json",
      "dev": "nodemon --exec \"pnpm run build && pnpm run dev:start\" --watch src --ext ts",
      "dev:start": "node ./dist/index.js",
      "lib:up": "pnpm update --latest",
      "lint": "eslint '{src, **/tests}/**/*.ts' && jest",
      "pretty": "prettier --write \"src/**/*.ts\"",
      "pretty:check": "prettier --check \"src/**/*.ts\"",
      "lint:fix": "eslint --fix '{src,**/tests}/**/*.ts'",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:coverage": "jest --coverage",
      "clean": "rm -rf dist"
   },
   "dependencies": {
      "@babel/core": "7.22.1",
      "@babel/node": "7.22.1",
      "@babel/preset-env": "7.22.2",
      "axios": "1.4.0",
      "express": "^4.18.2"
   },
   "devDependencies": {
      "@babel/cli": "7.21.5",
      "@eslint-config-typescript": "workspace:^",
      "@tsconfig": "workspace:^",
      "@types/compression": "1.7.2",
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
      "@types/hpp": "0.2.2",
      "@types/jest": "^29.5.1",
      "@types/node": "^20.1.0",
      "@types/supertest": "^2.0.12",
      "@types/uuid": "^9.0.1",
      "babel-plugin-module-resolver": "5.0.0",
      "compression": "1.7.4",
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "exifr": "^7.1.3",
      "hpp": "0.2.3",
      "husky": "^8.0.3",
      "jest": "^29.5.0",
      "kafkajs": "^2.2.4",
      "mongodb": "^5.4.0",
      "mongoose": "^7.1.0",
      "node-fetch": "^3.3.1",
      "nodemon": "^2.0.22",
      "prettier": "^2.8.8",
      "supertest": "^6.3.3",
      "ts-jest": "^29.1.0",
      "ts-node": "^10.9.1",
      "ts-patch": "^2.1.0",
      "typescript": "^5.0.4",
      "uuid": "^9.0.0",
      "winston": "3.9.0",
      "zod": "^3.21.4"
   }
}
