{
   "name": "@doFavour/identity-service",
   "version": "1.0.0",
   "private": true,
   "main": "build/index.js",
   "license": "UNLICENSED",
   "scripts": {
      "build": "tsc",
      "dev": "nodemon --exec \"pnpm run build && pnpm run dev:start\" --watch src --ext ts",
      "dev:start": "node ./build/index.js",
      "lib:up": "pnpm update --latest",
      "lint": "eslint '{src, **/tests}/**/*.ts' && jest",
      "pretty": "prettier --write \"src/**/*.ts\"",
      "pretty:check": "prettier --check \"src/**/*.ts\"",
      "lint:fix": "eslint --fix '{src,**/tests}/**/*.ts'",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:coverage": "jest --coverage",
      "clean": "rm -rf dist",
      "migrate": "prisma migrate dev --name init",
      "prisma:migrate": "prisma migrate dev --name init",
      "prisma:generate": "prisma generate",
      "prisma:studio": "prisma studio",
      "prisma:reset": "prisma migrate reset --force",
      "prisma:forget": "prisma migrate reset --force && prisma migrate dev --name init",
      "prisma:format": "prisma format"
   },
   "dependencies": {
      "@fastify/cors": "8.3.0",
      "@fastify/env": "4.2.0",
      "@prisma/client": "4.15.0",
      "fastify": "4.17.0",
      "fastify-plugin": "4.5.0",
      "fastify-zod": "1.3.2",
      "zod-to-json-schema": "3.21.1"
   },
   "devDependencies": {
      "@eslint-config-typescript": "workspace:^",
      "@rollup/plugin-typescript": "^11.1.1",
      "@types/compression": "1.7.2",
      "@types/node": "^20.2.5",
      "@types/supertest": "^2.0.12",
      "@types/uuid": "^9.0.1",
      "compression": "1.7.4",
      "cors": "^2.8.5",
      "dotenv": "^16.1.3",
      "nodemon": "2.0.22",
      "rollup": "^3.23.0",
      "rollup-plugin-dts": "^5.3.0",
      "ts-node": "10.9.1",
      "tslib": "2.5.3",
      "typescript": "^5.1.3",
      "uuid": "^9.0.0",
      "winston": "3.9.0",
      "zod": "^3.21.4"
   }
}
