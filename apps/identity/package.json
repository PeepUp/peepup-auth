{
   "name": "@doFavour/identity-service",
   "version": "1.0.0",
   "private": true,
   "main": "./dist/src/index.js",
   "license": "UNLICENSED",
   "scripts": {
      "build": "tsc",
      "dev": "nodemon --exec \"pnpm run build && pnpm run dev:start\" --watch src --ext ts",
      "dev:start": "node ./dist/index.js",
      "lint": "eslint '{src, **/tests}/**/*.ts' && jest",
      "pretty": "prettier --write \"src/**/*.ts\"",
      "pretty:check": "prettier --check \"src/**/*.ts\"",
      "lint:fix": "eslint --fix '{src,**/tests}/**/*.ts'",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:coverage": "jest --coverage",
      "clean": "rm -rf dist",
      "db:migrate": "npx prisma migrate dev --name init",
      "db:generate": "npx prisma generate",
      "db:studio": "npx prisma studio",
      "db:reset": "npx prisma migrate reset --force",
      "db:forget": "npx prisma migrate reset --force && prisma migrate dev --name init",
      "db:format": "npx prisma format",
      "db:seed": "npx prisma db seed"
   },
   "dependencies": {
      "@fastify/cors": "8.3.0",
      "@fastify/env": "4.2.0",
      "@prisma/client": "4.15.0",
      "fastify": "4.17.0",
      "fastify-plugin": "4.5.0",
      "fastify-zod": "1.3.2",
      "zod-to-json-schema": "3.21.1"
   },
   "prisma": {
      "seed": "ts-node ./prisma/seed.ts"
   },
   "devDependencies": {
      "@eslint-config-typescript": "workspace:^",
      "@types/compression": "1.7.2",
      "@types/node": "^20.2.5",
      "@types/supertest": "^2.0.12",
      "@types/jest": "^29.5.1",
      "supertest": "^6.3.3",
      "ts-jest": "^29.1.0",
      "ts-node": "^10.9.1",
      "ts-patch": "^2.1.0",
      "compression": "1.7.4",
      "jest": "^29.5.0",
      "cors": "^2.8.5",
      "dotenv": "^16.1.3",
      "nodemon": "2.0.22",
      "prisma": "4.15.0",
      "ts-node": "10.9.1",
      "tslib": "2.5.3",
      "typescript": "^5.1.3",
      "zod": "^3.21.4"
   }
}
