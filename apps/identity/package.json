{
    "name": "@dofavour/identity",
    "version": "1.0.0",
    "license": "MIT",
    "main": "./dist/src/index.js",
    "prisma": {
        "seed": "ts-node ./prisma/seed.ts"
    },
    "private": true,
    "author": "kbgjtn",
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf ./dist",
        "db:forget": "dotenv -e .env -- npx prisma migrate reset --force && prisma migrate dev --name init",
        "db:format": "dotenv -e .env -- npx prisma format",
        "db:generate": "dotenv -e .env -- npx prisma generate",
        "db:migrate": "dotenv -e .env -- npx prisma migrate dev --name init",
        "db:reset": "dotenv -e .env -- npx prisma migrate reset --force",
        "db:seed": "dotenv -e .env -- npx prisma db seed",
        "db:studio": "dotenv -e .env -- npx prisma studio",
        "dev": "dotenv -e .env -- npx nodemon",
        "lint": "eslint src --fix",
        "precommit": "lint-staged",
        "pretty": "prettier --write \"src/**/*.ts\"",
        "pretty:check": "prettier --check \"src/**/*.ts\"",
        "schema:generate": "NODE_ENV=development dotenv-flow -- tsx ./scripts/schema/index.script.ts",
        "start": "pnpm run build & node dist/index.js",
        "test": "jest --verbose --noCache --watchAll",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "@casl/ability": "6.5.0",
        "@casl/prisma": "1.4.1",
        "@fastify/autoload": "5.8.0",
        "@fastify/cookie": "9.2.0",
        "@fastify/cors": "8.5.0",
        "@prisma/client": "5.7.1",
        "argon2": "0.31.2",
        "casbin": "5.28.0",
        "casbin-prisma-adapter": "1.4.0",
        "dotenv": "16.3.1",
        "fastify": "4.25.1",
        "fastify-plugin": "4.5.1",
        "fastify-type-provider-zod": "1.1.9",
        "fastify-zod": "1.4.0",
        "jose": "5.1.3",
        "libphonenumber-js": "1.10.52",
        "nodemailer": "6.9.7"
    },
    "devDependencies": {
        "@dofavour/tsconfig": "workspace:*",
        "@fastify/static": "6.12.0",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.5",
        "@types/nodemailer": "6.4.14",
        "ajv": "8.12.0",
        "cors": "^2.8.5",
        "dotenv-cli": "7.3.0",
        "eslint": "8.56.0",
        "jest": "^29.7.0",
        "lint-staged": "15.2.0",
        "nodemon": "3.0.2",
        "prisma": "5.7.1",
        "rimraf": "5.0.5",
        "ts-jest": "^29.1.1",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "tslib": "2.6.2",
        "typescript": "^5.3.3",
        "typescript-transform-paths": "3.4.6",
        "zod": "^3.22.4"
    }
}
