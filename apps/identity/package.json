{
   "name": "@dofavour/identity",
   "version": "1.0.0",
   "private": true,
   "main": "./dist/src/index.js",
   "license": "MIT",
   "scripts": {
      "build": "tsc",
      "clean": "rm -rf ./dist",
      "start": "node dist/index.js",
      "dev": "NODE_ENV=development nodemon --exec \"pnpm run build && pnpm run dev:start\" --watch src --ext ts",
      "dev:start": "NODE_ENV=development node dist/index.js",
      "lint": "eslint --ext .ts src tests --no-error-on-unmatched-pattern --quiet || true",
      "schema:generate": "NODE_ENV=development dotenv-flow -- tsx ./scripts/schema/index.script.ts",
      "pretty": "prettier --write \"src/**/*.ts\"",
      "pretty:check": "prettier --check \"src/**/*.ts\"",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:coverage": "jest --coverage",
      "db:migrate": "npx prisma migrate dev --name init",
      "db:generate": "npx prisma generate",
      "db:studio": "npx prisma studio",
      "db:reset": "npx prisma migrate reset --force",
      "db:forget": "npx prisma migrate reset --force && prisma migrate dev --name init",
      "db:format": "npx prisma format",
      "db:seed": "npx prisma db seed",
      "precommit": "lint-staged"
   },
   "prisma": {
      "seed": "ts-node ./prisma/seed.ts"
   },
   "dependencies": {
      "@fastify/autoload": "5.7.1",
      "@fastify/cors": "8.3.0",
      "@prisma/client": "4.16.1",
      "argon2": "0.30.3",
      "fastify": "4.18.0",
      "fastify-plugin": "4.5.0",
      "fastify-zod": "1.3.2",
      "jose": "4.14.4"
   },
   "devDependencies": {
      "@dofavour/eslint-config-typescript": "workspace:*",
      "@dofavour/tsconfig": "workspace:*",
      "@types/jest": "^29.5.2",
      "@types/node": "^20.3.1",
      "@typescript-eslint/parser": "^5.60.0",
      "ajv": "8.12.0",
      "cors": "^2.8.5",
      "dotenv": "16.3.1",
      "fastify-static": "4.7.0",
      "fastify-type-provider-zod": "1.1.9",
      "jest": "^29.5.0",
      "libphonenumber-js": "1.10.36",
      "lint-staged": "13.2.2",
      "nodemon": "2.0.22",
      "prisma": "4.16.1",
      "ts-jest": "^29.1.0",
      "ts-node": "10.9.1",
      "tslib": "2.5.3",
      "typescript": "^5.1.3",
      "zod": "^3.21.4"
   }
}
