{
    "name": "@dofavour/identity",
    "version": "1.0.0",
    "license": "MIT",
    "main": "./dist/src/index.js",
    "prisma": {
        "seed": "ts-node ./prisma/seed.ts"
    },
    "private": true,
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf ./dist",
        "db:forget": "npx prisma migrate reset --force && prisma migrate dev --name init",
        "db:format": "npx prisma format",
        "db:generate": "npx prisma generate",
        "db:migrate": "npx prisma migrate dev --name init",
        "db:reset": "npx prisma migrate reset --force",
        "db:seed": "npx prisma db seed",
        "db:studio": "npx prisma studio",
        "dev": "NODE_ENV=development npx nodemon",
        "lint": "eslint src --fix",
        "precommit": "lint-staged",
        "pretty": "prettier --write \"src/**/*.ts\"",
        "pretty:check": "prettier --check \"src/**/*.ts\"",
        "schema:generate": "NODE_ENV=development dotenv-flow -- tsx ./scripts/schema/index.script.ts",
        "start": "pnpm run build & node dist/index.js",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "@casl/ability": "6.5.0",
        "@casl/prisma": "1.4.1",
        "@fastify/autoload": "5.7.1",
        "@fastify/cors": "8.3.0",
        "@prisma/client": "5.3.0",
        "argon2": "0.31.1",
        "dotenv": "16.3.1",
        "fastify": "4.23.1",
        "fastify-plugin": "4.5.1",
        "fastify-type-provider-zod": "1.1.9",
        "fastify-zod": "1.4.0",
        "jose": "4.14.6",
        "libphonenumber-js": "1.10.44"
    },
    "devDependencies": {
        "@dofavour/tsconfig": "workspace:*",
        "@types/jest": "^29.5.4",
        "@types/node": "^20.6.0",
        "ajv": "8.12.0",
        "cors": "^2.8.5",
        "eslint": "8.49.0",
        "fastify-static": "4.7.0",
        "jest": "^29.7.0",
        "lint-staged": "14.0.1",
        "nodemon": "3.0.1",
        "prisma": "5.3.0",
        "rimraf": "5.0.1",
        "ts-jest": "^29.1.1",
        "ts-node": "10.9.1",
        "tsconfig-paths": "4.2.0",
        "tslib": "2.6.2",
        "typescript": "^5.2.2",
        "typescript-transform-paths": "3.4.6",
        "zod": "^3.22.2"
    }
}
