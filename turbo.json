{
   "$schema": "https://turbo.build/schema.json",
   "globalDependencies": ["**/.env.*local"],
   "pipeline": {
      "build": {
         "dependsOn": ["^build"],
         "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
      },
      "lint": {},
      "check-types": {},
      "dev": {
         "cache": false,
         "persistent": true
      },
      "clean": {
         "cache": false
      },
      "db:generate": {
         "cache": false,
         "outputs": ["^generate"]
      },
      "db:format": {
         "cache": false,
         "outputs": ["^format"]
      },
      "db:migrate": {
         "cache": false,
         "outputs": ["^migrate"]
      },
      "db:seed": {
         "cache": false,
         "outputs": ["^seed"]
      }
   }
}
